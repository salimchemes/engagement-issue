<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.engagementissue943637" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>engagement-issue</name>
  <description>An awesome Ionic/Cordova app.</description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashMaintainAspectRatio" value="true"/>
  <preference name="FadeSplashScreenDuration" value="300"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
    <plugin name="cordova-plugin-ms-azure-mobile-engagement" spec="~3.0.1">
        <variable name="AZME_IOS_CONNECTION_STRING" value="Endpoint=" />
        <variable name="AZME_ANDROID_CONNECTION_STRING" value="Endpoint=" />
        <variable name="AZME_ANDROID_GOOGLE_PROJECT_NUMBER" value="" />
        <variable name="AZME_ENABLE_NATIVE_LOG" value="true" />
        <variable name="AZME_ENABLE_PLUGIN_LOG" value="true" />
    </plugin>
</widget>